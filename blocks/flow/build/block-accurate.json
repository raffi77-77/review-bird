{
  "apiVersion": 3,
  "name": "limb-ai/chatbot",
  "version": "0.1.0",
  "title": "Limb AI Chatbot",
  "category": "widgets",
  "icon": "format-chat",
  "description": "Block showing a Chatbot widget.",
  "attributes": {
    "preview": {
      "type": "boolean",
      "default": false
    },
    "chatbotID": {
      "type": "number",
      "default": 0
    },
    "sendMessagePlaceholder": {
      "type": "string",
      "default": "Send a message"
    },
    "sendButtonText": {
      "type": "string",
      "default": "Send"
    }
  },
  "example": {
    "attributes": {
      "preview": true,
      "chatbotID": 0,
      "sendMessagePlaceholder": "Send a message",
      "sendButtonText": "Send"
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js",
  "textdomain": "limb-ai",
  "keywords": [
    "limb",
    "ai",
    "chat",
    "bot",
    "chatbot"
  ],
  "supports": {
    "html": false
  }
}